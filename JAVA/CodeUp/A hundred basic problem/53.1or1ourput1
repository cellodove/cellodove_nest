
import java.util.Scanner;

public class main {


    public static void main(String[] args) {

        Scanner SC = new Scanner(System.in);
        int A = SC.nextInt();
        int B = SC.nextInt();
        if (A == 1 || B == 1) {

            System.out.print(1);
        } else {
            System.out.print(0);
        }
        SC.close();
    }
}
