
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner SN = new Scanner(System.in);

        int A = SN.nextInt();
        int B = SN.nextInt();

        if (B != A) {

            System.out.print(1);
        } else {
            System.out.print(0);

        }


    }


}
