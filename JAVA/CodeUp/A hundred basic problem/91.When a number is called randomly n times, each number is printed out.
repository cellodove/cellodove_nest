
import java.util.Arrays;
import java.util.Scanner;

public class main {

    public static void main(String[] args) {

        Scanner SC = new Scanner(System.in);
        int RR = SC.nextInt();
        int CLASS[] = new int[24];
        Arrays.fill(CLASS,0);
        int BAN;

        int i;
        for (i=0; i<RR; i++){
            BAN = SC.nextInt();
            CLASS[BAN] += 1;
        }

        for(i=1; i<CLASS.length; i++){
            System.out.print(CLASS[i]+" ");
        }
    }
}
