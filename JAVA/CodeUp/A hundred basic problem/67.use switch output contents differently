import java.util.Scanner;

public class main {

    public static void main(String[] args) {

        Scanner SC = new Scanner(System.in);
        String A = SC.next();

        switch (A) {

            case ("A"):
                System.out.print("best!!!");
                break;
            case ("B"):
                System.out.print("good!!");
                break;
            case ("C"):
                System.out.print("run!");
                break;
            case ("D"):
                System.out.print("slowly~");
                break;
            default:
                System.out.print("what?");

        }
    }

}
