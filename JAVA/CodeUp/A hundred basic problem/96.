
import java.util.Scanner;

public class main {
    public static void main(String[] args) {

        //격자판 입력된 세로 가로로 만들기
        Scanner SC = new Scanner(System.in);
        int h = SC.nextInt();
        int w = SC.nextInt();
        int Grid[][] = new int[h][w];

        //막대의 갯수 입력
        int n = SC.nextInt();


        //막대가 만들어지는 부분
        for(int i = 0; i==n; i++){
            int l = SC.nextInt(); //막대의 길이
            int d = SC.nextInt(); //막대의 방향 가로는 0 세로는 1
            int x = SC.nextInt(); // 막대의 x좌표
            int y = SC.nextInt(); //막대의 y좌표

            if(d==0){
                //int stick1[][]= new int[h][w];
                for(int j=y; j<l; j++){
                    Grid[x][y]=1;
                }
            }
            else if(d==1){
                //int stick1[][]= new int[h][w];
                for(int j=x; j<l; j++){
                    Grid[x][y]=1;
                }
            }
        }
        System.out.print("\n");
        for(int z=0; z<Grid.length; z++){
            for(int q=0; q<Grid.length; q++){
                System.out.printf("%d ",Grid[z][q]);
            }
            System.out.print("\n");
        }
    }
}
