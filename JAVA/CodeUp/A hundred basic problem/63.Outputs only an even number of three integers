
import java.util.Scanner;

public class main {

    public static void main(String[] args) {


        Scanner SC = new Scanner(System.in);
        int A = SC.nextInt();
        int B = SC.nextInt();
        int C = SC.nextInt();

        if (A % 2 == 0) {
            System.out.println(A );
        }
        if (B % 2 == 0) {
            System.out.println(B );
        }
        if (C % 2 == 0) {
            System.out.println(C );
        }


    }


}
