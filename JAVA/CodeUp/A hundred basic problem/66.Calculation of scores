import java.util.Scanner;

public class main {

    public static void main(String[] args) {

        Scanner SC = new Scanner(System.in);
        int A = SC.nextInt();

        if (A >= 90 & A <= 100) {
            System.out.print("A");
        } else if (A >= 70 & A <= 89) {
            System.out.print("B");
        } else if (A >= 40 & A <= 69) {
            System.out.print("C");
        } else if (A >= 0 & A <= 39) {
            System.out.print("D");
        }
    }

}
