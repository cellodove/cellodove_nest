import java.util.Scanner;

public class main {
    public static void main(String[] args) {

        Scanner SC = new Scanner(System.in);
        System.out.print("더할 두수를 입력하세요.: ");
        int A = SC.nextInt();
        int B = SC.nextInt();

        System.out.printf("%d와 %d의 합은 %d입니다.",A,B,A+B);
    }
}
